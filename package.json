{
    "name": "vscode-xmind-viewer",
    "publisher": "local-test",
    "displayName": "XMind Viewer",
    "description": "Preview and Edit Multi-sheet XMind files in VS Code (with fixed UI)",
    "version": "0.0.5",
    "engines": {
        "vscode": "^1.85.0"
    },
    "categories": [
        "Visualization"
    ],
    "activationEvents": [
        "onCustomEditor:vscode-xmind-viewer.xmindEditor"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "customEditors": [
            {
                "viewType": "vscode-xmind-viewer.xmindEditor",
                "displayName": "XMind Viewer",
                "selector": [
                    {
                        "filenamePattern": "*.xmind"
                    }
                ],
                "priority": "default"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run package",
        "compile": "webpack",
        "watch": "webpack --watch",
        "package": "webpack --mode production --devtool hidden-source-map",
        "compile-tests": "tsc -p . --outDir out",
        "watch-tests": "tsc -p . -w --outDir out",
        "pretest": "npm run compile-tests && npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^8.1.0",
        "@types/mocha": "^10.0.6",
        "@types/node": "20.x",
        "@types/vscode": "^1.85.0",
        "@typescript-eslint/eslint-plugin": "^6.15.0",
        "@typescript-eslint/parser": "^6.15.0",
        "css-loader": "^7.1.2",
        "eslint": "^8.56.0",
        "glob": "^8.1.0",
        "mocha": "^10.2.0",
        "style-loader": "^4.0.0",
        "ts-loader": "^9.5.1",
        "typescript": "^5.3.3",
        "webpack": "^5.89.0",
        "webpack-cli": "^5.1.4"
    },
    "dependencies": {
        "@types/uuid": "^10.0.0",
        "jszip": "^3.10.1",
        "simple-mind-map": "^0.14.0-fix.1",
        "uuid": "^13.0.0"
    }
}